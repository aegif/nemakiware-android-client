<?xml version="1.0" encoding="utf-8"?>
<PreferenceScreen xmlns:android="http://schemas.android.com/apk/res/android"  android:key="mainPreferences">
	<PreferenceCategory android:title="Data">
		<CheckBoxPreference 
			android:key="cache"
			android:title="Delete cache" 
			android:summary="Allow activer Params"
			android:defaultValue="false" />
		<EditTextPreference 
			android:key="cache_directory"
			android:title="Change Directory" 
			android:summary="Directory where we store feed and document cache."
			android:defaultValue="-1" />
		<EditTextPreference 
			android:key="@string/cmis_repo_filter"
			android:title="Filter" 
			android:summary="Filter"
			android:defaultValue="*" />
		<EditTextPreference 
			android:key="@string/cmis_repo_types"
			android:title="Types" 
			android:summary="Types"
			android:defaultValue="" />
		<EditTextPreference 
			android:key="@string/cmis_repo_orderby"
			android:title="Order By" 
			android:summary="Order By"
			android:defaultValue="cmis:name ASC" />
	</PreferenceCategory>
</PreferenceScreen>
